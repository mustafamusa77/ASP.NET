@model List<WebApplication1.Models.UserModel>

    <h1>Users:</h1>
    <table class="table">
        <thead>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </thead>
        <tbody>
            @foreach(var user in Model)
            {
               <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
               </tr>
            }
        </tbody>
    </table>